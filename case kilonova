Codul SursÄƒ:
#include <bits/stdc++.h>

using namespace std;

vector<int> v;
stack<int>s ;

int main()
{
    int n,i;
    cin>>n;
    v.resize(n);
    for(auto& i:v)
    {
        cin>>i;
    }
    v.resize(n+1);
    for(i=n;i>=1;i--)
    {
        v[i]=v[i-1];
    }
    for(i=1;i<=n;i++)
    {
        while(s.empty()==false && v[i]<=v[s.top()])
        {
            s.pop();
        }
        if(s.empty()==true)
        {
            cout<<0<<" ";
        }
        else if(v[i]>v[s.top()])
        {
            cout<<s.top()<<" ";
        }
        s.push(i);
    }
    return 0;
}
